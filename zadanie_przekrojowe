def wyznacz_pierwsze(liczba):
    pierwsze = [2]

    def czy_pierwsza(liczba):
        for dzielnik in pierwsze:
            if liczba % dzielnik == 0:
                return False
            if dzielnik * dzielnik > liczba:
                return True
        return True

    for i in range(2, liczba):
        if czy_pierwsza(i) is True:
            pierwsze.append(i)
    return pierwsze

print(wyznacz_pierwsze(300))